page{
  width: 100%;
  height: 100%;
}

.box{
  width: 100%;
  height: 100%;
}

.homePage {
  width: 100%;
  height: 100%;
  position: relative;                                   
  display: flex;
  flex-direction: column;
} 

.buttons{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.3); 
  position: absolute;
  top: 0;
  left: 0;
  color: #fff;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  align-items: center;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.btn1{
  position: absolute;
  left: 40rpx;
  bottom: 100rpx;
}

.btn2{
  position: absolute;
  right: 40rpx;
  bottom: 100rpx;
}

